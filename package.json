{
  "name": "jc-markdown",
  "author": {
    "name": "John Chritton",
    "email": "jchritton33@gmail.com",
    "url": "https://github.com/jchritton"
  },
  "version": "1.0.0",
  "description": "React application previewing the Github-flavored output of Markdown.",
  "private": true,
  "homepage": "https://jchritton.github.io/jc-markdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/jchritton/jc-markdown"
  },
  "bugs": {
    "url": "https://github.com/jchritton/jc-markdown/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sass": "node-sass-chokidar --output-style expanded --source-map-embed true src/sass/style.scss -o src/css",
    "watch:sass": "node-sass-chokidar --output-style expanded --source-map-embed true src/sass -o src/css --watch --recursive"
  },
  "dependencies": {
    "gh-pages": "^2.0.1",
    "marked": "^0.5.1",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "^2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass-chokidar": "^1.3.4"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "marked",
    "markdown",
    "sass",
    "freecodecamp"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
