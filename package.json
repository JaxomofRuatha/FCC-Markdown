{
  "name": "jc-markdown",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://jchritton.github.io/jc-markdown/",
  "dependencies": {
    "gh-pages": "^1.1.0",
    "marked": "^0.3.19",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sass":
      "node-sass-chokidar --output-style expanded --source-map-embed true src/sass/style.scss -o src/css",
    "watch:sass":
      "node-sass-chokidar --output-style expanded --source-map-embed true src/sass -o src/css --watch --recursive"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "node-sass-chokidar": "^1.3.0"
  }
}
